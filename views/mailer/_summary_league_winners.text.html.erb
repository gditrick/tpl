<table style="<%= table_default %>">
<caption style="<%= caption_default %>">League Winners</caption>
<thead>
  <tr>
    <th style="<%= th_default %>">Prize</th>
    <th style="<%= th_default %>">Period</th>
    <th style="<%= th_default %>">Winner</th>
    <th style="<%= th_default %>">Won</th>
  </tr>
</thead>
<tbody>
<%= render :partial => "summary_league_winner.text.html",
  :collection => winnings,
  :as => :winning,
  :locals => { :team => team } %>
</tbody>
<tfoot>
  <tr>
    <th colspan="3" style="<%= th_default + th_total %>">Total:</th>
    <%= league_winnings_total_col_detail_html(winnings) %>
  </tr>
</tfoot>
</table>
